{
  "name": "@upscalerjs/esrgan-medium",
  "version": "1.0.0-beta.10",
  "description": "ESRGAN Medium Model for UpscalerJS. Upscale images and increase image resolution with AI using Javascript",
  "keywords": [
    "image enhancement",
    "super resolution",
    "upscaling",
    "tensorflowjs",
    "neural network",
    "ai",
    "esrgan"
  ],
  "author": "Kevin Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/upscaler.git"
  },
  "exports": {
    "./2x": {
      "require": "./dist/cjs/models/esrgan-medium/src/2x.js",
      "import": "./dist/esm/models/esrgan-medium/src/2x.js"
    },
    "./3x": {
      "require": "./dist/cjs/models/esrgan-medium/src/3x.js",
      "import": "./dist/esm/models/esrgan-medium/src/3x.js"
    },
    "./4x": {
      "require": "./dist/cjs/models/esrgan-medium/src/4x.js",
      "import": "./dist/esm/models/esrgan-medium/src/4x.js"
    },
    "./8x": {
      "require": "./dist/cjs/models/esrgan-medium/src/8x.js",
      "import": "./dist/esm/models/esrgan-medium/src/8x.js"
    },
    ".": {
      "require": "./dist/cjs/models/esrgan-medium/src/index.js",
      "import": "./dist/esm/models/esrgan-medium/src/index.js"
    }
  },
  "scripts": {
    "scaffold:dependencies": "ts-node-esm ../../scripts/package-scripts/scaffold-dependencies.mts --src models/esrgan-medium --config models/scaffolder.mts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.cjs src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate:build",
    "validate:build": "ts-node-esm ../../scripts/package-scripts/validate-build.mts models/esrgan-medium",
    "build": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-medium -o cjs -o esm -o umd",
    "build:cjs": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-medium -o cjs",
    "build:esm": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-medium -o esm",
    "build:umd": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-medium -o umd",
    "clean": "ts-node-esm ../../scripts/package-scripts/clean-model.ts esrgan-medium"
  },
  "files": [
    "assets/**/*",
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "~4.8.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "~4.8.0",
    "@tensorflow/tfjs-layers": "~4.8.0",
    "@tensorflow/tfjs": "~4.8.0",
    "@tensorflow/tfjs-node": "~4.8.0",
    "@tensorflow/tfjs-node-gpu": "~4.8.0",
    "seedrandom": "3.0.5"
  },
  "@upscalerjs": {
    "title": "ESRGAN Medium",
    "modelFamily": "esrgan",
    "models": {
      "./8x": {
        "supportedPlatforms": [
          "node"
        ]
      }
    }
  }
}
