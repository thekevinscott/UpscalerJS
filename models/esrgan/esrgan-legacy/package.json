{
  "name": "@upscalerjs/esrgan-legacy",
  "version": "1.0.0-beta.11",
  "description": "ESRGAN Legacy Models for Upscaler.js",
  "exports": {
    "./div2k/2x": {
      "require": "./dist/cjs/models/esrgan-legacy/src/div2k/2x.js",
      "import": "./dist/esm/models/esrgan-legacy/src/div2k/2x.js"
    },
    "./div2k/3x": {
      "require": "./dist/cjs/models/esrgan-legacy/src/div2k/3x.js",
      "import": "./dist/esm/models/esrgan-legacy/src/div2k/3x.js"
    },
    "./div2k/4x": {
      "require": "./dist/cjs/models/esrgan-legacy/src/div2k/4x.js",
      "import": "./dist/esm/models/esrgan-legacy/src/div2k/4x.js"
    },
    "./psnr-small": {
      "require": "./dist/cjs/models/esrgan-legacy/src/psnr-small.js",
      "import": "./dist/esm/models/esrgan-legacy/src/psnr-small.js"
    },
    "./gans": {
      "require": "./dist/cjs/models/esrgan-legacy/src/gans.js",
      "import": "./dist/esm/models/esrgan-legacy/src/gans.js"
    },
    ".": {
      "require": "./dist/cjs/models/esrgan-legacy/src/index.js",
      "import": "./dist/esm/models/esrgan-legacy/src/index.js"
    }
  },
  "scripts": {
    "scaffold:dependencies": "ts-node ../../../scripts/package-scripts/scaffold-dependencies.ts --src models/esrgan-legacy --config models/scaffolder.ts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate:build",
    "validate:build": "ts-node ../../../scripts/package-scripts/validate-build.ts models/esrgan-legacy",
    "build": "ts-node ../../../scripts/package-scripts/build-model.ts esrgan-legacy -o cjs -o esm -o umd",
    "build:cjs": "pnpm --filter @upscalerjs/scripts model:build esrgan-legacy -o cjs",
    "build:esm": "pnpm --filter @upscalerjs/scripts model:build esrgan-legacy -o esm",
    "build:umd": "pnpm --filter @upscalerjs/scripts model:build esrgan-legacy -o umd",
    "clean": "pnpm --filter @upscalerjs/scripts model:clean esrgan-legacy"
  },
  "keywords": [
    "image super resolution",
    "image upscaling",
    "image enhancement",
    "tensorflow.js",
    "pretrained models",
    "esrgan"
  ],
  "files": [
    "assets/**/*",
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "~4.8.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "~4.8.0",
    "@tensorflow/tfjs-layers": "~4.8.0",
    "@tensorflow/tfjs": "~4.8.0",
    "@tensorflow/tfjs-node": "~4.8.0",
    "@tensorflow/tfjs-node-gpu": "~4.8.0",
    "seedrandom": "3.0.5"
  },
  "author": "Kevin Scott",
  "license": "MIT"
}
