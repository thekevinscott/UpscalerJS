{
  "name": "@upscalerjs/esrgan-experiments",
  "version": "0.1.0",
  "description": "A folder for various ESRGAN experimental models",
  "exports": {
    "./2x/small": {
      "require": "./dist/cjs/2x/small.js",
      "import": "./dist/esm/2x/small.js"
    },
    "./2x/medium": {
      "require": "./dist/cjs/2x/medium.js",
      "import": "./dist/esm/2x/medium.js"
    },
    "./2x/large": {
      "require": "./dist/cjs/2x/large.js",
      "import": "./dist/esm/2x/large.js"
    },
    "./3x/small": {
      "require": "./dist/cjs/3x/small.js",
      "import": "./dist/esm/3x/small.js"
    },
    "./3x/medium": {
      "require": "./dist/cjs/3x/medium.js",
      "import": "./dist/esm/3x/medium.js"
    },
    "./3x/large": {
      "require": "./dist/cjs/3x/large.js",
      "import": "./dist/esm/3x/large.js"
    },
    "./4x/small": {
      "require": "./dist/cjs/4x/small.js",
      "import": "./dist/esm/4x/small.js"
    },
    "./4x/medium": {
      "require": "./dist/cjs/4x/medium.js",
      "import": "./dist/esm/4x/medium.js"
    },
    "./4x/large": {
      "require": "./dist/cjs/4x/large.js",
      "import": "./dist/esm/4x/large.js"
    },
    "./8x/small": {
      "require": "./dist/cjs/8x/small.js",
      "import": "./dist/esm/8x/small.js"
    },
    "./8x/medium": {
      "require": "./dist/cjs/8x/medium.js",
      "import": "./dist/esm/8x/medium.js"
    },
    "./8x/large": {
      "require": "./dist/cjs/8x/large.js",
      "import": "./dist/esm/8x/large.js"
    },
    ".": {		
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "scaffold:dependencies": "ts-node ../../scripts/package-scripts/scaffold-dependencies.ts --src models/esrgan-experiments --config models/scaffolder.ts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate:build",
    "validate:build": "ts-node ../../scripts/package-scripts/validate-build.ts models/esrgan-experiments",
    "build": "ts-node ../../scripts/package-scripts/build-model.ts esrgan-experiments -o cjs -o esm -o umd"
  },
  "keywords": [
    "image super resolution",
    "image upscaling",
    "image enhancement",
    "tensorflow.js",
    "pretrained models",
    "esrgan"
  ],
  "files": [
    "license",
    "src/**/*",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "^3.20.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "^3.20.0",
    "@tensorflow/tfjs-layers": "^3.20.0",
    "@tensorflow/tfjs": "^3.20.0",
    "@tensorflow/tfjs-node": "^3.20.0",
    "@tensorflow/tfjs-node-gpu": "^3.20.0",
    "seedrandom": "3.0.5"
  },
  "author": "Kevin Scott",
  "license": "MIT"
}
