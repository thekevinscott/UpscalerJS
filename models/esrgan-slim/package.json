{
  "name": "@upscalerjs/esrgan-slim",
  "version": "1.0.0-beta.9",
  "description": "ESRGAN Slim Model for UpscalerJS. Upscale images and increase image resolution with AI using Javascript",
  "keywords": [
    "image enhancement",
    "super resolution",
    "upscaling",
    "tensorflowjs",
    "neural network",
    "ai",
    "esrgan"
  ],
  "author": "Kevin Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/upscaler.git"
  },
  "exports": {
    "./2x": {
      "require": "./dist/cjs/models/esrgan-slim/src/2x.js",
      "import": "./dist/esm/models/esrgan-slim/src/2x.js"
    },
    "./3x": {
      "require": "./dist/cjs/models/esrgan-slim/src/3x.js",
      "import": "./dist/esm/models/esrgan-slim/src/3x.js"
    },
    "./4x": {
      "require": "./dist/cjs/models/esrgan-slim/src/4x.js",
      "import": "./dist/esm/models/esrgan-slim/src/4x.js"
    },
    "./8x": {
      "require": "./dist/cjs/models/esrgan-slim/src/8x.js",
      "import": "./dist/esm/models/esrgan-slim/src/8x.js"
    },
    ".": {
      "require": "./dist/cjs/models/esrgan-slim/src/index.js",
      "import": "./dist/esm/models/esrgan-slim/src/index.js"
    }
  },
  "scripts": {
    "scaffold:dependencies": "wireit",
    "lint:fix": "wireit",
    "lint": "wireit",
    "prepublishOnly": "wireit",
    "validate:build": "wireit",
    "build": "wireit",
    "build:cjs": "wireit",
    "build:esm": "wireit",
    "build:umd": "wireit",
    "clean": "wireit"
  },
  "files": [
    "assets/**/*",
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "~4.8.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "~4.8.0",
    "@tensorflow/tfjs-layers": "~4.8.0",
    "@tensorflow/tfjs": "~4.8.0",
    "@tensorflow/tfjs-node": "~4.8.0",
    "@tensorflow/tfjs-node-gpu": "~4.8.0",
    "seedrandom": "3.0.5"
  },
  "@upscalerjs": {
    "title": "ESRGAN Slim",
    "modelFamily": "esrgan",
    "models": {
      "./8x": {
        "supportedPlatforms": [
          "node"
        ]
      }
    }
  },
  "wireit": {
    "scaffold:dependencies": {
      "command": "ts-node ../../scripts/package-scripts/scaffold-dependencies.ts --src models/esrgan-slim --config models/scaffolder.ts"
    },
    "lint:fix": {
      "command": "pnpm lint --fix"
    },
    "lint": {
      "command": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.js src --ext .ts"
    },
    "prepublishOnly": {
      "command": "pnpm lint && pnpm build && pnpm validate:build"
    },
    "validate:build": {
      "command": "ts-node ../../scripts/package-scripts/validate-build.ts models/esrgan-slim"
    },
    "build": {
      "command": "pnpm --filter @upscalerjs/scripts model:build esrgan-slim -o cjs -o esm -o umd"
    },
    "build:cjs": {
      "command": "pnpm --filter @upscalerjs/scripts model:build esrgan-slim -o cjs"
    },
    "build:esm": {
      "command": "pnpm --filter @upscalerjs/scripts model:build esrgan-slim -o esm"
    },
    "build:umd": {
      "command": "pnpm --filter @upscalerjs/scripts model:build esrgan-slim -o umd"
    },
    "clean": {
      "command": "pnpm --filter @upscalerjs/scripts model:clean esrgan-slim"
    }
  }
}