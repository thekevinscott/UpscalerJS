{
  "name": "@upscalerjs/esrgan-slim",
  "version": "1.0.0-beta.9",
  "description": "ESRGAN Slim Model for UpscalerJS. Upscale images and increase image resolution with AI using Javascript",
  "keywords": [
    "image enhancement",
    "super resolution",
    "upscaling",
    "tensorflowjs",
    "neural network",
    "ai",
    "esrgan"
  ],
  "author": "Kevin Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/upscaler.git"
  },
  "exports": {
    "./2x": {
      "require": "./dist/cjs/models/esrgan-slim/src/2x.js",
      "import": "./dist/esm/models/esrgan-slim/src/2x.js"
    },
    "./3x": {
      "require": "./dist/cjs/models/esrgan-slim/src/3x.js",
      "import": "./dist/esm/models/esrgan-slim/src/3x.js"
    },
    "./4x": {
      "require": "./dist/cjs/models/esrgan-slim/src/4x.js",
      "import": "./dist/esm/models/esrgan-slim/src/4x.js"
    },
    "./8x": {
      "require": "./dist/cjs/models/esrgan-slim/src/8x.js",
      "import": "./dist/esm/models/esrgan-slim/src/8x.js"
    },
    ".": {
      "require": "./dist/cjs/models/esrgan-slim/src/index.js",
      "import": "./dist/esm/models/esrgan-slim/src/index.js"
    }
  },
  "scripts": {
    "scaffold": "pnpm cli scaffold model esrgan-slim",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold && eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate",
    "validate": "pnpm cli validate models/esrgan-slim",
    "build": "pnpm cli build model esrgan-slim -o cjs -o esm -o umd",
    "build:cjs": "pnpm cli build model esrgan-slim -o cjs",
    "build:esm": "pnpm cli build model esrgan-slim -o esm",
    "build:umd": "pnpm cli build model esrgan-slim -o umd"
  },
  "files": [
    "assets/**/*",
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "~4.8.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "~4.8.0",
    "@tensorflow/tfjs-layers": "~4.8.0",
    "@tensorflow/tfjs": "~4.8.0",
    "@tensorflow/tfjs-node": "~4.8.0",
    "@tensorflow/tfjs-node-gpu": "~4.8.0",
    "seedrandom": "3.0.5"
  },
  "@upscalerjs": {
    "title": "ESRGAN Slim",
    "modelFamily": "esrgan",
    "models": {
      "./8x": {
        "supportedPlatforms": [
          "node"
        ]
      }
    }
  }
}
