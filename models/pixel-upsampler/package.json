{
  "name": "@upscalerjs/pixel-upsampler",
  "version": "1.0.0-beta.6",
  "description": "Pixel Upsampler Model for Upscaler.js",
  "scripts": {
    "scaffold:dependencies": "ts-node ../../scripts/package-scripts/scaffold-dependencies.ts --src models/pixel-upsampler --config models/scaffolder.ts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate:build",
    "validate:build": "ts-node ../../scripts/package-scripts/validate-build.ts models/pixel-upsampler",
    "build": "ts-node ../../scripts/package-scripts/build-model.ts pixel-upsampler -o cjs -o esm -o umd"
  },
  "exports": {
    "./2x": {
      "require": {
        "types": "./dist/cjs/2x.d.ts",
        "default": "./dist/cjs/2x.js"
      },
      "import": {
        "types": "./dist/esm/2x.d.ts",
        "default": "./dist/esm/2x.js"
      }
    },
    "./3x": {
      "require": {
        "types": "./dist/cjs/3x.d.ts",
        "default": "./dist/cjs/3x.js"
      },
      "import": {
        "types": "./dist/esm/3x.d.ts",
        "default": "./dist/esm/3x.js"
      }
    },
    "./4x": {
      "require": {
        "types": "./dist/cjs/4x.d.ts",
        "default": "./dist/cjs/4x.js"
      },
      "import": {
        "types": "./dist/esm/4x.d.ts",
        "default": "./dist/esm/4x.js"
      }
    },
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "keywords": [
    "image super resolution",
    "image upscaling",
    "image enhancement",
    "tensorflow.js",
    "pretrained models"
  ],
  "files": [
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "^3.19.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-core": "^3.19.0",
    "@tensorflow/tfjs-layers": "^3.19.0",
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^3.19.0",
    "@tensorflow/tfjs-node": "^3.19.0",
    "@tensorflow/tfjs-node-gpu": "^3.19.0",
    "seedrandom": "3.0.5"
  },
  "author": "Kevin Scott",
  "license": "MIT"
}
