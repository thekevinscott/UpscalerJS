{
  "name": "@upscalerjs/pixel-upsampler",
  "version": "0.4.5",
  "description": "Pixel Upsampler Model for Upscaler.js",
  "dependencies": {
    "@tensorflow/tfjs-core": "^3.18.0",
    "@tensorflow/tfjs-layers": "^3.18.0",
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "seedrandom": "2.4.4"
  },
  "scripts": {
    "lint:fix": "eslint -c ../.eslintrc.js src --ext .ts --fix",
    "lint": "eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build",
    "build": "ts-node ../../scripts/package-scripts/build-model.ts pixel-upsampler -o cjs -o esm -o umd"
  },
  "exports": {
    "./2x": {
      "require": "./dist/node/2x.js",
      "import": "./dist/browser/esm/2x.js"
    },
    "./3x": {
      "require": "./dist/node/3x.js",
      "import": "./dist/browser/esm/3x.js"
    },
    "./4x": {
      "require": "./dist/node/4x.js",
      "import": "./dist/browser/esm/4x.js"
    },
    ".": {
      "require": "./dist/node/2x.js",
      "import": "./dist/browser/esm/2x.js"
    }
  },
  "keywords": [
    "image super resolution",
    "image upscaling",
    "image enhancement",
    "tensorflow.js",
    "pretrained models"
  ],
  "files": [
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "author": "Kevin Scott",
  "license": "MIT"
}
