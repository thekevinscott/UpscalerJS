{
  "name": "@upscalerjs/pixel-upsampler",
  "version": "1.0.0-beta.10",
  "description": "Pixel Upsampler Model for UpscalerJS",
  "scripts": {
    "scaffold:dependencies": "ts-node-esm ../../scripts/package-scripts/scaffold-dependencies.mts --src models/pixel-upsampler --config models/scaffolder.mts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.cjs src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate:build",
    "validate:build": "ts-node-esm ../../scripts/package-scripts/validate-build.mts models/pixel-upsampler",
    "build": "ts-node-esm ../../scripts/package-scripts/build-model.mts pixel-upsampler -o cjs -o esm -o umd"
  },
  "exports": {
    "./2x": {
      "require": "./dist/cjs/2x.js",
      "import": "./dist/esm/2x.js"
    },
    "./3x": {
      "require": "./dist/cjs/3x.js",
      "import": "./dist/esm/3x.js"
    },
    "./4x": {
      "require": "./dist/cjs/4x.js",
      "import": "./dist/esm/4x.js"
    },
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "assets/**/*",
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "~4.8.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-core": "~4.8.0",
    "@tensorflow/tfjs-layers": "~4.8.0",
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "~4.8.0",
    "@tensorflow/tfjs-node": "~4.8.0",
    "@tensorflow/tfjs-node-gpu": "~4.8.0",
    "seedrandom": "3.0.5"
  },
  "author": "Kevin Scott",
  "license": "MIT"
}
