    "@tensorflow/tfjs": "^4.6.0",
    "@tensorflow/tfjs-node": "^4.6.0",
    "@tensorflow/tfjs-node-gpu": "^4.6.0",
    "seedrandom": "3.0.5"
  },
  "@upscalerjs": {
    "model": {
      "experimental": true
    },
    "assets": {
