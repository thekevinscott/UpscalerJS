{
  "name": "@upscalerjs/esrgan-legacy",
  "version": "1.0.0-beta.3",
  "description": "ESRGAN Legacy Models for Upscaler.js",
  "scripts": {
    "scaffold:dependencies": "ts-node ../../scripts/package-scripts/scaffold-dependencies.ts --src models/esrgan-legacy --config models/scaffolder.ts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build",
    "build": "ts-node ../../scripts/package-scripts/build-model.ts esrgan-legacy -o cjs -o esm -o umd"
  },
  "exports": {
    "./div2k/2x": {
      "require": {
        "types": "./dist/cjs/div2k/2x.d.ts",
        "default": "./dist/cjs/div2k/2x.js"
      },
      "import": {
        "types": "./dist/esm/div2k/2x.d.ts",
        "default": "./dist/esm/div2k/2x.js"
      }
    },
    "./div2k/3x": {
      "require": {
        "types": "./dist/cjs/div2k/3x.d.ts",
        "default": "./dist/cjs/div2k/3x.js"
      },
      "import": {
        "types": "./dist/esm/div2k/3x.d.ts",
        "default": "./dist/esm/div2k/3x.js"
      }
    },
    "./div2k/4x": {
      "require": {
        "types": "./dist/cjs/div2k/4x.d.ts",
        "default": "./dist/cjs/div2k/4x.js"
      },
      "import": {
        "types": "./dist/esm/div2k/4x.d.ts",
        "default": "./dist/esm/div2k/4x.js"
      }
    },
    "./psnr-small": {
      "require": {
        "types": "./dist/cjs/psnr-small.d.ts",
        "default": "./dist/cjs/psnr-small.js"
      },
      "import": {
        "types": "./dist/esm/psnr-small.d.ts",
        "default": "./dist/esm/psnr-small.js"
      }
    },
    "./gans": {
      "require": {
        "default": "./dist/cjs/gans.js",
        "types": "./dist/cjs/gans.d.ts"
      },
      "import": {
        "default": "./dist/esm/gans.js",
        "types": "./dist/esm/gans.d.ts"
      }
    }
  },
  "keywords": [
    "image super resolution",
    "image upscaling",
    "image enhancement",
    "tensorflow.js",
    "pretrained models",
    "esrgan"
  ],
  "files": [
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "^3.18.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "^3.18.0",
    "@tensorflow/tfjs-layers": "^3.18.0",
    "@tensorflow/tfjs": "^3.18.0",
    "@tensorflow/tfjs-node": "^3.18.0",
    "@tensorflow/tfjs-node-gpu": "^3.18.0",
    "seedrandom": "2.4.4"
  },
  "author": "Kevin Scott",
  "license": "MIT"
}
