{
  "name": "@upscalerjs/esrgan-legacy",
  "version": "0.3.1",
  "description": "ESRGAN Legacy Models for Upscaler.js",
  "dependencies": {
    "@tensorflow/tfjs-core": "^3.18.0",
    "@tensorflow/tfjs-layers": "^3.18.0",
    "@upscalerjs/core": "workspace:*"
  },
  "exports": {
    "./div2k/2x": {
      "require": {
        "default": "./dist/node/div2k/2x.js",
        "types": "./dist/node/div2k/2x.d.ts"
      },
      "import": {
        "default": "./dist/browser/esm/div2k/2x.js",
        "types": "./dist/browser/esm/div2k/2x.d.ts"
      }
    },
    "./div2k/3x": {
      "require": {
        "default": "./dist/node/div2k/3x.js",
        "types": "./dist/node/div2k/3x.d.ts"
      },
      "import": {
        "default": "./dist/browser/esm/div2k/3x.js",
        "types": "./dist/browser/esm/div2k/3x.d.ts"
      }
    },
    "./div2k/4x": {
      "require": {
        "default": "./dist/node/div2k/4x.js",
        "types": "./dist/node/div2k/4x.d.ts"
      },
      "import": {
        "default": "./dist/browser/esm/div2k/4x.js",
        "types": "./dist/browser/esm/div2k/4x.d.ts"
      }
    },
    "./psnr-small": {
      "require": {
        "default": "./dist/node/psnr-small.js",
        "types": "./dist/node/psnr-small.d.ts"
      },
      "import": {
        "default": "./dist/browser/esm/psnr-small.js",
        "types": "./dist/browser/esm/psnr-small.d.ts"
      }
    },
    "./gans": {
      "require": {
        "types": "./dist/node/gans.d.ts",
        "default": "./dist/node/gans.js"
      },
      "import": {
        "types": "./dist/browser/esm/gans.d.ts",
        "default": "./dist/browser/esm/gans.js"
      }
    }
  },
  "scripts": {
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.js src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build",
    "build": "ts-node ../../scripts/package-scripts/build-model.ts esrgan-legacy -o cjs -o esm -o umd"
  },
  "keywords": [
    "image super resolution",
    "image upscaling",
    "image enhancement",
    "tensorflow.js",
    "pretrained models",
    "esrgan"
  ],
  "files": [
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "devDependencies": {
    "seedrandom": "2.4.4"
  },
  "author": "Kevin Scott",
  "license": "MIT"
}
