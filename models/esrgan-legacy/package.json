{
  "name": "@upscalerjs/esrgan-legacy",
  "version": "1.0.0-beta.11",
  "description": "ESRGAN Legacy Models for UpscalerJS. Upscale images and increase image resolution with AI using Javascript",
  "keywords": [
    "image enhancement",
    "super resolution",
    "upscaling",
    "tensorflowjs",
    "neural network",
    "ai",
    "esrgan"
  ],
  "author": "Kevin Scott",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/upscaler.git"
  },
  "exports": {
    "./div2k/2x": {
      "require": "./dist/cjs/models/esrgan-legacy/src/div2k/2x.js",
      "import": "./dist/esm/models/esrgan-legacy/src/div2k/2x.js"
    },
    "./div2k/3x": {
      "require": "./dist/cjs/models/esrgan-legacy/src/div2k/3x.js",
      "import": "./dist/esm/models/esrgan-legacy/src/div2k/3x.js"
    },
    "./div2k/4x": {
      "require": "./dist/cjs/models/esrgan-legacy/src/div2k/4x.js",
      "import": "./dist/esm/models/esrgan-legacy/src/div2k/4x.js"
    },
    "./psnr-small": {
      "require": "./dist/cjs/models/esrgan-legacy/src/psnr-small.js",
      "import": "./dist/esm/models/esrgan-legacy/src/psnr-small.js"
    },
    "./gans": {
      "require": "./dist/cjs/models/esrgan-legacy/src/gans.js",
      "import": "./dist/esm/models/esrgan-legacy/src/gans.js"
    },
    ".": {
      "require": "./dist/cjs/models/esrgan-legacy/src/index.js",
      "import": "./dist/esm/models/esrgan-legacy/src/index.js"
    }
  },
  "scripts": {
    "scaffold:dependencies": "ts-node-esm ../../scripts/package-scripts/scaffold-dependencies.mts --src models/esrgan-legacy --config models/scaffolder.mts",
    "lint:fix": "pnpm lint --fix",
    "lint": "pnpm scaffold:dependencies && eslint -c ../.eslintrc.cjs src --ext .ts",
    "prepublishOnly": "pnpm lint && pnpm build && pnpm validate:build",
    "validate:build": "ts-node-esm ../../scripts/package-scripts/validate-build.mts models/esrgan-legacy",
    "build": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-legacy -o cjs -o esm -o umd",
    "build:cjs": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-legacy -o cjs",
    "build:esm": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-legacy -o esm",
    "build:umd": "ts-node-esm ../../scripts/package-scripts/build-model.mts esrgan-legacy -o umd",
    "clean": "ts-node-esm ../../scripts/package-scripts/clean-model.ts esrgan-legacy"
  },
  "files": [
    "assets/**/*",
    "license",
    "models/**/*",
    "dist/**/*"
  ],
  "peerDependencies": {
    "@tensorflow/tfjs": "~4.8.0"
  },
  "dependencies": {
    "@upscalerjs/core": "workspace:*"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "~4.8.0",
    "@tensorflow/tfjs-layers": "~4.8.0",
    "@tensorflow/tfjs": "~4.8.0",
    "@tensorflow/tfjs-node": "~4.8.0",
    "@tensorflow/tfjs-node-gpu": "~4.8.0",
    "seedrandom": "3.0.5"
  }
}
