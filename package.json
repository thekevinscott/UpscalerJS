{
  "name": "upscaler",
  "version": "0.7.7",
  "description": "Increase image resolution with a Neural Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "super resolution",
    "image",
    "scale",
    "resize",
    "esrgan",
    "upscaling",
    "image enhancement",
    "image super resolution",
    "tensorflow.js",
    "tfjs"
  ],
  "files": [
    "LICENSE",
    "dist/**/*",
    "models/**/*"
  ],
  "scripts": {
    "docs:serve": "docsify serve docs --port 3030",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "bump_version": "yarn format && git add -A src && node scripts/bump_version.js",
    "build": "rimraf dist && tsc --skipLibCheck",
    "watch": "rimraf dist && tsc --watch --skipLibCheck",
    "format": "echo 'Formatting code' && prettier --write \"src/**/*.ts\"",
    "lint": "echo 'Linting code' && tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "test:integration:browser:ci": "jest test/test.js --ci",
    "test:integration:browser": "jest test/test.js --detectOpenHandles"
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/UpscalerJS.git"
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thekevinscott/UpscalerJS/issues"
  },
  "homepage": "https://github.com/thekevinscott/UpscalerJS#readme",
  "peerDependencies": {
    "@tensorflow/tfjs": "2.8.6"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "2.8.6",
    "@types/jest": "^26.0.7",
    "browserstack-cypress-cli": "^1.7.1",
    "browserstack-local": "^1.4.8",
    "codecov": "^3.7.2",
    "concurrently": "^6.0.0",
    "cypress": "^6.5.0",
    "docsify-cli": "^4.4.2",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "selenium-webdriver": "^4.0.0-beta.1",
    "serve": "^11.3.2",
    "ts-jest": "^26.1.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "dot-prop": "^5.1.1",
    "tensor-as-base64": "^0.1.1"
  }
}
