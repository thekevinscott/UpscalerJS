{
  "name": "upscalerjs-root",
  "version": "1.0.0",
  "private": true,
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "scripts": {
    "postinstall": "pnpm --filter upscaler run scaffold:platform 'browser'",
    "update:tfjs": "ts-node scripts/package-scripts/update-tfjs.ts",
    "update:version": "ts-node scripts/package-scripts/update-version.ts",
    "commit-latest-version-changes": "git add packages/upscalerjs/src/constants.ts *examples/*/package.json* && git commit -m \"Updating CDN && example versions\" --no-verify",
    "version": "pnpm run pin-cdn",
    "update:version:old": "node ./scripts/version.js",
    "pin-cdn": "node scripts/pin-cdn.js",
    "find-all-packages": "ts-node scripts/package-scripts/find-all-packages.ts",
    "example": "ts-node ./scripts/start-example.ts",
    "lint": "pnpm --filter upscaler --filter @upscalerjs/* lint",
    "test:integration:browserstack": "ts-node scripts/test.ts --platform browser --kind browserstack",
    "test:integration:browser": "ts-node scripts/test.ts --platform browser",
    "test:integration:node": "ts-node scripts/test.ts --platform node",
    "test:start-test-server": "ts-node test/lib/startServer.ts",
    "test:memory-leaks": "ts-node scripts/check-memory-leaks.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/UpscalerJS.git"
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "homepage": "https://github.com/thekevinscott/UpscalerJS",
  "peerDependencies": {
    "@tensorflow/tfjs": "^3.12.0"
  },
  "devDependencies": {
    "upscaler": "workspace:*",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@tensorflow/tfjs": "^3.13.0",
    "@tensorflow/tfjs-node": "^3.13.0",
    "@tensorflow/tfjs-node-gpu": "^3.13.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^28.0.0",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.0",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/puppeteer": "^5.4.4",
    "@types/rimraf": "^3.0.2",
    "@types/serve-handler": "^6.1.1",
    "@types/yargs": "^17.0.5",
    "browserstack-local": "^1.4.8",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chokidar": "^3.5.3",
    "codecov": "^3.7.2",
    "docsify-cli": "^4.4.3",
    "dotenv": "^8.2.0",
    "esbuild": "^0.9.6",
    "fs-extra": "^10.1.0",
    "heapdump": "^0.3.15",
    "html-webpack-plugin": "^5.3.0",
    "inquirer": "^8.2.0",
    "jest": "^28.0.0",
    "jest-canvas-mock": "^2.2.0",
    "jsdom": "^19.0.0",
    "loader-utils": "^2.0.0",
    "mocha": "^9.2.1",
    "pixelmatch": "^5.2.1",
    "playwright-test": "^7.2.2",
    "pngjs": "^6.0.0",
    "prettier": "^2.0.5",
    "puppeteer": "^13.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "seedrandom": "~2.4.3",
    "selenium-webdriver": "^4.2.0",
    "serve": "^11.3.2",
    "tape": "^5.5.0",
    "tree-kill": "^1.2.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.6.0",
    "typescript": "^4.7.4",
    "uglify-js": "^3.15.4",
    "webpack": "^5.24.3",
    "yargs": "^17.3.1"
  },
  "packageManager": "pnpm@3.1.0",
  "dependencies": {
    "@types/selenium-webdriver": "^4.1.1"
  }
}
