{
  "name": "@upscalerjs/root",
  "description": "This is the root package.json for the UpscalerJS monorepo. You are probably looking for the package.json located at packages/upscalerjs/package.json",
  "author": "Kevin Scott",
  "license": "MIT",
  "homepage": "https://github.com/thekevinscott/UpscalerJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/UpscalerJS.git"
  },
  "scripts": {
    "clidev": "pnpm --filter @internals/cli clidev --",
    "lint": "pnpm --filter upscaler --filter @upscalerjs/* lint",
    "dev:browser": "pnpm --filter=\"@upscalerjs/dev-browser\" run dev",
    "dev:node": "pnpm --filter=\"@upscalerjs/dev-node\" run dev",
    "preinstall": "npx only-allow pnpm",
    "liteinstall": "pnpm --filter=!\"@upscalerjs/docs\" --filter=!\"@upscalerjs/examples.*\" install",
    "postinstall": "wireit"
  },
  "wireit": {
    "postinstall": {
      "command": "pnpm --filter upscaler run scaffold 'browser' && pnpm --filter @upscalerjs/core --filter @upscalerjs/default-model build",
      "dependencies": [
        "./internals/cli:build"
      ]
    }
  },
  "type": "module",
  "private": true,
  "peerDependencies": {
    "@tensorflow/tfjs": "4.8.0",
    "@tensorflow/tfjs-node": "4.8.0",
    "@tensorflow/tfjs-node-gpu": "4.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.10",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-transform-modules-commonjs": "7.22.5",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-typescript": "7.22.5",
    "@internals/bundlers": "workspace:*",
    "@internals/cli": "workspace:*",
    "@internals/common": "workspace:*",
    "@internals/http-server": "workspace:*",
    "@internals/test-runner": "workspace:*",
    "@internals/webdriver": "workspace:*",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-node-resolve": "15.2.0",
    "@schemastore/package": "0.0.10",
    "@swc/core": "^1.3.81",
    "@tensorflow/tfjs": "4.8.0",
    "@tensorflow/tfjs-node": "4.8.0",
    "@tensorflow/tfjs-node-gpu": "4.8.0",
    "@types/babel__core": "7.20.1",
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "7.1.5",
    "@types/fs-extra": "11.0.1",
    "@types/inquirer": "9.0.3",
    "@types/jsdom": "21.1.1",
    "@types/mocha": "10.0.1",
    "@types/node": "20.5.1",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@types/serve-handler": "6.1.1",
    "@types/tar": "6.1.5",
    "@types/tiny-async-pool": "2.0.0",
    "@types/yargs": "17.0.24",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "browserstack-local": "1.5.4",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "chokidar": "3.5.3",
    "docsify-cli": "4.4.4",
    "dotenv": "16.3.1",
    "esbuild": "0.19.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsdoc": "46.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "fast-folder-size": "2.2.0",
    "fs-extra": "11.1.1",
    "heapdump": "0.3.15",
    "html-webpack-plugin": "5.5.3",
    "inquirer": "9.2.10",
    "jsdom": "22.1.0",
    "mocha": "10.2.0",
    "pixelmatch": "5.3.0",
    "playwright-test": "12.1.1",
    "pngjs": "7.0.0",
    "prettier": "3.0.2",
    "puppeteer": "^21.1.0",
    "rimraf": "5.0.1",
    "rollup": "3.28.0",
    "seedrandom": "3.0.5",
    "serve": "14.2.0",
    "serve-handler": "6.1.5",
    "tape": "5.6.6",
    "tar": "6.1.15",
    "tiny-async-pool": "2.1.0",
    "tree-kill": "1.2.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "uglify-js": "3.17.4",
    "upscaler": "workspace:*",
    "vite": "4.4.9",
    "vitest": "^0.34.3",
    "webpack": "5.88.2",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "pnpm": {
    "overrides": {
      "@tensorflow/tfjs": "4.8.0",
      "@tensorflow/tfjs-node": "4.8.0",
      "@tensorflow/tfjs-node-gpu": "4.8.0"
    }
  },
  "dependencies": {
    "tsx": "^3.12.10",
    "wireit": "^0.14.0"
  }
}
