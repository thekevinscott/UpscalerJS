{
  "version": "0.8.17",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "format": "lerna run format",
    "lint": "lerna run lint",
    "push": "lerna publish --conventional-commits --create-release=github",
    "pin-cdn": "node scripts/pin-cdn.js && yarn commit-latest-version-changes",
    "commit-latest-version-changes": "git add packages/upscalerjs/src/constants.ts *examples/*/package.json* && git commit -m \"Updating CDN && example versions\" --no-verify",
    "version": "yarn pin-cdn",
    "postversion": "lerna run build",
    "bootstrap": "lerna bootstrap --hoist"
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/UpscalerJS.git"
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "homepage": "https://github.com/thekevinscott/UpscalerJS",
  "peerDependencies": {
    "@tensorflow/tfjs": "2.8.6"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "2.8.6",
    "@types/jest": "^26.0.7",
    "babel-jest": "^26.6.3",
    "browserstack-cypress-cli": "^1.7.1",
    "browserstack-local": "^1.4.8",
    "codecov": "^3.7.2",
    "concurrently": "^6.0.0",
    "docsify-cli": "^4.4.2",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^5.3.0",
    "image-to-base64": "^2.1.1",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.2.0",
    "loader-utils": "^2.0.0",
    "parcel": "^2.0.0-beta.1",
    "pixelmatch": "^5.2.1",
    "playwright": "^1.9.1",
    "pngjs": "^6.0.0",
    "lerna": "^3.14.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "selenium-webdriver": "^4.0.0-beta.1",
    "serve": "^11.3.2",
    "ts-jest": "^26.1.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "uglify": "^0.1.5",
    "webpack": "^5.24.3",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "tensor-as-base64": "^0.1.1"
  }
}
