{
  "name": "@upscalerjs/root",
  "version": "1.0.0-beta.0",
  "private": true,
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "scripts": {
    "postinstall": "pnpm --filter upscaler run scaffold:platform 'browser' && pnpm --filter @upscalerjs/core build",
    "update:tfjs": "ts-node scripts/package-scripts/update-tfjs.ts",
    "update:version": "ts-node scripts/package-scripts/update-version.ts",
    "commit-latest-version-changes": "git add packages/upscalerjs/src/constants.ts *examples/*/package.json* && git commit -m \"Updating CDN && example versions\" --no-verify",
    "version": "pnpm run pin-cdn",
    "update:version:old": "node ./scripts/version.js",
    "pin-cdn": "node scripts/pin-cdn.js",
    "find-all-packages": "ts-node scripts/package-scripts/find-all-packages.ts",
    "example": "ts-node ./scripts/start-example.ts",
    "lint": "pnpm --filter upscaler --filter @upscalerjs/* lint",
    "test:integration:browserstack": "ts-node scripts/test.ts --platform browser --kind browserstack",
    "test:integration:browser": "ts-node scripts/test.ts --platform browser",
    "test:integration:node": "ts-node scripts/test.ts --platform node",
    "test:start-test-server": "ts-node test/lib/startServer.ts",
    "test:memory-leaks": "ts-node scripts/check-memory-leaks.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/UpscalerJS.git"
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "homepage": "https://github.com/thekevinscott/UpscalerJS",
  "peerDependencies": {
    "@tensorflow/tfjs": "^3.18.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@tensorflow/tfjs": "^3.18.0",
    "@tensorflow/tfjs-node": "^3.18.0",
    "@tensorflow/tfjs-node-gpu": "^3.18.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.4",
    "@types/jsdom": "^16.2.14",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/puppeteer": "^5.4.6",
    "@types/rimraf": "^3.0.2",
    "@types/serve-handler": "^6.1.1",
    "@types/yargs": "^17.0.10",
    "browserstack-local": "^1.5.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chokidar": "^3.5.3",
    "docsify-cli": "^4.4.4",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.48",
    "fs-extra": "^10.1.0",
    "heapdump": "^0.3.15",
    "html-webpack-plugin": "^5.5.0",
    "inquirer": "^8.2.4",
    "jest": "^28.1.2",
    "jest-canvas-mock": "^2.4.0",
    "jsdom": "^20.0.0",
    "mocha": "^10.0.0",
    "pixelmatch": "^5.3.0",
    "playwright-test": "^8.1.1",
    "pngjs": "^6.0.0",
    "prettier": "^2.7.1",
    "puppeteer": "^15.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "selenium-webdriver": "^4.3.0",
    "serve": "^11.3.2",
    "tape": "^5.5.3",
    "tree-kill": "^1.2.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "uglify-js": "^3.16.1",
    "upscaler": "workspace:*",
    "webpack": "^5.73.0",
    "yargs": "^17.5.1"
  },
  "packageManager": "pnpm@3.1.0",
  "dependencies": {
    "@types/mocha": "^9.1.1",
    "@types/selenium-webdriver": "^4.1.1",
    "seedrandom": "~2.4.3"
  }
}
