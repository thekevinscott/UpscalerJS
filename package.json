{
  "version": "0.8.27",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "scripts": {
    "example": "node ./scripts/start_example.js",
    "update:version": "node ./scripts/version.js",
    "format": "yarn lerna run format",
    "lint": "yarn lerna run lint",
    "push": "yarn lerna publish from-package",
    "push_old": "yarn lerna publish --conventional-commits --create-release=github",
    "pin-cdn": "node scripts/pin-cdn.js && yarn commit-latest-version-changes",
    "commit-latest-version-changes": "git add packages/upscalerjs/src/constants.ts *examples/*/package.json* && git commit -m \"Updating CDN && example versions\" --no-verify",
    "version": "yarn pin-cdn",
    "postversion": "yarn build",
    "bootstrap": "yarn lerna bootstrap --hoist",
    "build": "yarn lerna run build",
    "build:cjs": "yarn lerna run build:cjs",
    "build:umd": "yarn lerna run build:umd",
    "build:esm": "yarn lerna run build:esm",
    "watch:esm": "yarn lerna run watch:esm",
    "server": "yarn serve -p 8000 packages/upscalerjs/test/lib/webpack-bundler/dist/",
    "set:tfjs": "yarn lerna run set:tfjs",
    "test": "yarn test:unit",
    "test:unit": "jest --config packages/upscalerjs/jestconfig.json",
    "test:integration:browser": "jest test/integration/browser --config test/jestconfig.json --detectOpenHandles",
    "test:integration:node": "jest test/integration/node --config test/jestconfig.json --detectOpenHandles",
    "test:startTestServer": "ts-node test/lib/startServer.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekevinscott/UpscalerJS.git"
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "homepage": "https://github.com/thekevinscott/UpscalerJS",
  "peerDependencies": {
    "@tensorflow/tfjs": "2.8.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@tensorflow/tfjs": "2.8.6",
    "@tensorflow/tfjs-node": "2.8.6",
    "@tensorflow/tfjs-node-gpu": "2.8.6",
    "@types/jest": "^26.0.7",
    "@types/pngjs": "^6.0.1",
    "@types/rimraf": "^3.0.2",
    "@types/serve-handler": "^6.1.1",
    "@types/yargs": "^17.0.5",
    "browserstack-local": "^1.4.8",
    "codecov": "^3.7.2",
    "docsify-cli": "^4.4.2",
    "dotenv": "^8.2.0",
    "esbuild": "^0.9.6",
    "html-webpack-plugin": "^5.3.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.2.0",
    "lerna": "^3.14.0",
    "loader-utils": "^2.0.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "selenium-webdriver": "^4.0.0-beta.1",
    "serve": "^11.3.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7",
    "uglify": "^0.1.5",
    "webpack": "^5.24.3",
    "yargs": "^16.2.0"
  },
  "dependencies": {
    "tensor-as-base64": "^0.1.1"
  },
  "packageManager": "yarn@3.1.0"
}
