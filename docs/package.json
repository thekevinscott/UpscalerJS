{
  "name": "@upscalerjs/docs",
  "version": "1.0.0-beta.10",
  "private": true,
  "scripts": {
    "build:dependencies": "pnpm --filter @upscalerjs/scripts model:build -a -o cjs -o esm -o umd && pnpm --filter upscaler build",
    "write-api": "pnpm cli write docs api",
    "write-api:watch": "nodemon -e ts --ignore '../packages/upscalerjs/**/*.generated.ts' --watch '../packages/core/**/*' --watch '../packages/upscalerjs/**/*' --watch '../internals/cli/src/lib/write/docs/api/**/*' -x \"pnpm run write-api\"",
    "write-guides": "pnpm cli write docs guides",
    "write-guides:watch": "nodemon -e md --watch '../examples/**/*' -x \"pnpm run write-guides\"",
    "write-model-readmes": "pnpm cli write docs models",
    "write-model-readmes:watch": "nodemon -e md -e mdx --watch '../models/**/*' -x \"pnpm run write-model-readmes\"",
    "docusaurus": "docusaurus",
    "start": "pnpm run start:all",
    "start:all": "concurrently \"pnpm run docusaurus:start\" \"pnpm run write-model-readmes:watch\" \"pnpm run write-guides:watch\" \"pnpm run write-api:watch\"",
    "docusaurus:start": "docusaurus start --host 0.0.0.0",
    "build:only": "docusaurus build",
    "build": "concurrently \"pnpm write-guides --should-clear-markdown\" \"pnpm write-api --should-clear-markdown\" \"pnpm write-model-readmes --should-clear-markdown\" && pnpm tense-checks && pnpm build:dependencies && pnpm build:only",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "tense-checks": "pnpm --filter @upscalerjs/scripts docs:tense-checks",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@docusaurus/theme-common": "^2.4.1",
    "@mdx-js/react": "^1.6.2",
    "@shoelace-style/shoelace": "2.7.0",
    "@tensorflow/tfjs": "~4.8.0",
    "@upscalerjs/esrgan-medium": "workspace:*",
    "chart.js": "^4.3.3",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.30.0",
    "google-palette": "^1.1.1",
    "image-comparison-viewer": "0.5.4",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.0.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-github-btn": "^1.4.0",
    "react-icons": "^4.10.1",
    "react-loading": "^2.0.3",
    "react-markdown": "^8.0.7",
    "react-twitter-embed": "^4.0.4",
    "seedrandom": "3.0.5",
    "sql-wasm": "^1.1.4",
    "sql.js": "^1.8.0",
    "stream-browserify": "^3.0.0",
    "upscaler": "workspace:*"
  },
  "devDependencies": {
    "@internals/common": "workspace:*",
    "@algolia/client-search": "^4.19.1",
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@docusaurus/types": "^2.4.1",
    "@tsconfig/docusaurus": "^2.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/sql.js": "^1.4.4",
    "concurrently": "^8.2.0",
    "docusaurus-plugin-sass": "^0.2.5",
    "docusaurus-plugin-typedoc": "^0.19.2",
    "esbuild": "^0.19.2",
    "esbuild-loader": "^4.0.1",
    "nodemon": "^3.0.1",
    "prop-types": "^15.8.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "wireit": {
    "build:dependencies": {
      "command": "pnpm --filter upscaler build",
      "dependencies": [
        "../models/esrgan-medium:build"
      ]
    },
    "build-api": {
      "command": "pnpm --filter @upscalerjs/scripts docs:build-api",
      "dependencies": [
        "../internals/common:build"
      ]
    },
    "build-api:watch": {
      "command": "nodemon -e ts --ignore '../packages/upscalerjs/**/*.generated.ts' --watch '../packages/core/**/*' --watch '../packages/upscalerjs/**/*' --watch '../scripts/package-scripts/docs/build-api.ts' -x \"pnpm run build-api\""
    },
    "build-guides": {
      "command": "pnpm --filter @upscalerjs/scripts docs:build-guides"
    },
    "build-guides:watch": {
      "command": "nodemon -e md --watch '../examples/**/*' --watch '../scripts/package-scripts/docs/build-guides.ts' -x \"pnpm run build-guides\""
    },
    "link-model-readmes": {
      "command": "pnpm --filter @upscalerjs/scripts docs:link-model-readmes"
    },
    "link-model-readmes:watch": {
      "command": "nodemon -e md -e mdx --watch '../models/**/*' --watch '../scripts/package-scripts/docs/link-model-readmes.ts' -x \"pnpm run link-model-readmes\""
    },
    "docusaurus": {
      "command": "docusaurus"
    },
    "start": {
      "command": "pnpm run start:all"
    },
    "start:all": {
      "command": "concurrently \"pnpm run docusaurus:start\" \"pnpm run link-model-readmes:watch\" \"pnpm run build-guides:watch\" \"pnpm run build-api:watch\""
    },
    "docusaurus:start": {
      "command": "docusaurus start --host 0.0.0.0"
    },
    "build:only": {
      "command": "docusaurus build"
    },
    "build": {
      "command": "concurrently \"pnpm build-guides -- --shouldClearMarkdown\" \"pnpm build-api -- --shouldClearMarkdown\" \"pnpm link-model-readmes -- --shouldClearMarkdown\" && pnpm tense-checks && pnpm build:dependencies && pnpm build:only"
    },
    "swizzle": {
      "command": "docusaurus swizzle"
    },
    "deploy": {
      "command": "docusaurus deploy"
    },
    "clear": {
      "command": "docusaurus clear"
    },
    "serve": {
      "command": "docusaurus serve"
    },
    "write-translations": {
      "command": "docusaurus write-translations"
    },
    "write-heading-ids": {
      "command": "docusaurus write-heading-ids"
    },
    "tense-checks": {
      "command": "pnpm --filter @upscalerjs/scripts docs:tense-checks"
    },
    "typecheck": {
      "command": "tsc"
    }
  }
}
