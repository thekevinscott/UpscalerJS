{
  "name": "@upscalerjs/docs",
  "version": "1.0.0-beta.8",
  "private": true,
  "scripts": {
    "build:dependencies": "pnpm --filter @upscalerjs/scripts model:build -a -o cjs -o esm -o umd && pnpm --filter upscaler build",
    "link-model-readmes": "pnpm --filter @upscalerjs/scripts docs:link-model-readmes",
    "link-model-readmes:watch": "nodemon -e md -e mdx --watch '../models/**/*' -x \"pnpm run link-model-readmes\"",
    "docusaurus": "docusaurus",
    "start": "pnpm build:dependencies && pnpm run start:all",
    "start:all": "concurrently \"pnpm run docusaurus:start\" \"pnpm run link-model-readmes:watch\"",
    "docusaurus:start": "docusaurus start --host 0.0.0.0",
    "build": "pnpm build:dependencies && pnpm link-model-readmes && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@docusaurus/theme-common": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "@shoelace-style/shoelace": "2.0.0-beta.80",
    "@upscalerjs/core": "workspace:*",
    "chart.js": "^4.0.1",
    "classnames": "^2.3.1",
    "clsx": "^1.2.1",
    "crypto-browserify": "^3.12.0",
    "google-palette": "^1.1.0",
    "image-comparison-viewer": "^0.4.2",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-chartjs-2": "^5.0.1",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.0.2",
    "react-github-btn": "^1.3.0",
    "react-icons": "^4.4.0",
    "react-loading": "^2.0.3",
    "sql-wasm": "^1.1.4",
    "sql.js": "^1.8.0",
    "stream-browserify": "^3.0.0",
    "tensor-as-base64": "^0.3.0",
    "upscaler": "workspace:*"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@docusaurus/module-type-aliases": "2.2.0",
    "@docusaurus/types": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/chart.js": "^2.9.37",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.9",
    "@types/react-icons": "^3.0.0",
    "@types/sql.js": "^1.4.4",
    "concurrently": "^7.3.0",
    "docusaurus-plugin-sass": "^0.2.2",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "nodemon": "^2.0.20",
    "prop-types": "^15.8.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.0.2",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.8.3",
    "webpack": "^5.73.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
