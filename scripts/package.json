    "test:integration:browserstack": "pnpm _script_ ./test.ts --platform browser --kind browserstack",
    "test:integration:browser": "pnpm _script_ ./test.ts --platform browser",
    "test:integration:node": "pnpm _script_ ./test.ts --platform node",
    "test:memory-leaks": "pnpm _script_ ./check-memory-leaks.ts"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "inquirer": "^8.2.4",
    "@types/yargs": "^17.0.10",
    "@types/fs-extra": "^9.0.13",
