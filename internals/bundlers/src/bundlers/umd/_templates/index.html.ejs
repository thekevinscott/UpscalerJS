<html>
  <head>
    <link rel="shortcut icon" href="data:image/x-icon;," type="image/x-icon"> 
    <title>UpscalerJS Integration Test: UMD</title>
  </head>
  <body>
    <h1 id="output">Loading...</h1>
    <script src="https://cdn.jsdelivr.net/npm/@tensorflow/tfjs@<%- tfjsVersion %>/dist/tf.min.js"></script>
    <script src="/upscaler.min.js"></script>
    <script type="text/javascript">
      window['loadScript'] = function loadScript(src) {
        return new Promise((resolve, reject) => {
          const script = document.createElement('script');
          script.setAttribute("type", "text/javascript");
          script.setAttribute("src", src);
          document.getElementsByTagName("head")[0].appendChild(script);
          script.onload = () => resolve(src);
          script.onerror = reject;
        });
      };
      document.title = [document.title, 'Loaded'].join(' | ');
      document.body.querySelector('#output').innerHTML = document.title;
    </script>
  </body>
</html>
