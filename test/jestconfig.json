{
  "preset": "ts-jest",
  "transform": {
     "^.+\\.(ts|tsx)?$": "ts-jest",
     "^.+\\.(js|jsx)$": "babel-jest"
  },
  "setupFiles": ["./jest.setup.js"],
  "collectCoverage": false,
  "testRegex": "(/integration/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}
